# ClawGateway Docker Compose Environment
# Copy this to .env and fill in your values, or run: ./docker/generate-env.sh

# --- Gateway ---
GATEWAY_PORT=8422
SESSION_SECRET=generate-a-random-secret-at-least-32-chars
CALLBACK_URL=http://localhost:8422/auth/callback
NODE_ENV=production

# --- SSO Provider (configure at least one) ---
AUTH_PROVIDER=okta
AUTH_ISSUER=https://dev-XXXXX.okta.com
AUTH_CLIENT_ID=your-okta-client-id
AUTH_CLIENT_SECRET=your-okta-client-secret

# --- Admin ---
ADMIN_EMAIL=admin@example.com

# --- Dev Mode (bypasses SSO, auto-login as devUser) ---
DEV_MODE=false
DEV_USER_EMAIL=admin@test.local
DEV_USER_ROLE=admin

# --- OpenClaw Gateway Tokens (auto-generated by generate-env.sh) ---
OPENCLAW_TOKEN_VIEWER=change-me-viewer-token
OPENCLAW_TOKEN_MEMBER=change-me-member-token
OPENCLAW_TOKEN_ADMIN=change-me-admin-token

# --- AI Provider ---
ANTHROPIC_API_KEY=
DEFAULT_MODEL=anthropic/claude-sonnet-4-5
